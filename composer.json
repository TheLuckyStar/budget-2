{
    "name": "simondubois/budget",
    "description": "Online envelope system money to keep a budget under control",
    "keywords": ["accountability", "app", "web"],
    "license": "GPL-2.0",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "doctrine/dbal": "^2.5",
        "barryvdh/laravel-debugbar": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
